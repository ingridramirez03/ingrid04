<%- include ('header') -%>
<%- include ('navbar') -%>
<%- include ('footer') -%>

<div class="container mt-2">

    <form>

        <div class="form-group row">
            <label for="nombre" class="col-sm-3 col-form-label">Nombre</label>
            <div class="col-sm-9">
                <input type="text" id="nombre" nombre="nombre" class="form-control"
                    placeholder="Ingrese su nombre y apellido...">
        
            </div>
        </div>
        
        <div class="form-group row">
            <label for="email" class="col-sm-3 col-form-label">Email</label>
            <div class="col-sm-9">
                <input type="email" id="email" nombre="email" class="form-control" placeholder="email@example.com">
            </div>
        </div>
        
        <div class="form-group row">
            <label for="comentario" class="col-sm-3 col-form-label">Comentario/Sugerencia</label>
            <div class="col-sm-9">
                <textarea type="text" id="comentario" nombre="comentario" class="form-control"
                    placeholder="Ingrese su comentario..." rows="15"></textarea>
            </div>
        </div>
        <div class="text-center mb-2">
            <button type="button" class="btn btn-outline-primary"><i class="fas fa-paper-plane"></i>Enviar</button>
        
        </div>




      </form>



</div>

<p style="height: 1000px;"></p>
<%- include ('pie') -%>